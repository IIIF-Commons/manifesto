{
  "name": "manifesto.js",
  "version": "3.0.9-pr-42",
  "description": "IIIF Presentation API utility library for client and server",
  "main": "./dist-esmodule/index.js",
  "types": "./dist-esmodule/index.d.ts",
  "scripts": {
    "clean": "npx rimraf dist-umd dist-commonjs dist-esmodule",
    "build": "npm run clean && tsc && tsc -m es6 --outDir dist-esmodule && webpack",
