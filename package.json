  },
  "homepage": "https://github.com/iiif-commons/manifesto",
  "devDependencies": {
    "@types/node": "10.7.1",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "2.3.0",
    "finalhandler": "0.4.0",
    "http": "0.0.0",
    "mocha": "2.2.4",
    "rimraf": "^2.6.2",
    "serve-static": "1.10.0",
    "typedoc": "^0.12.0",
    "typescript": "3.0.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "yargs": "3.10.0"
  },
  "dependencies": {
