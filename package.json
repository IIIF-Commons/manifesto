{
  "name": "manifesto.js",
  "version": "4.2.14",
  "description": "IIIF Presentation API utility library for client and server",
  "main": "./dist/bundle/cjs/index.js",
  "module": "./dist/bundle/esm/index.mjs",
  "browser": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    ".build",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/bundle/cjs/index.js",
      "import": "./dist/bundle/esm/index.mjs",
      "default": "./dist/index.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "watch": "node ./scripts/watch.mjs",
    "start": "vite",
    "test": "vitest",
    "prepublishOnly": "node ./scripts/build.mjs"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=3.10.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iiif-commons/manifesto.git"
  },
  "keywords": [
    "IIIF"
  ],
  "author": "@edsilv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iiif-commons/manifesto/issues"
  },
  "homepage": "https://github.com/iiif-commons/manifesto",
  "devDependencies": {
    "@types/node": "16.18.3",
    "dts-bundle-generator": "^7.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4",
    "vite": "^3.2.2"
  },
  "dependencies": {
    "@edsilv/http-status-codes": "^1.0.3",
    "@iiif/parser": "^1.1.0",
    "@iiif/vocabulary": "^1.0.26",
    "chalk": "^5.1.2",
    "execa": "^6.1.0",
    "happy-dom": "^7.6.6",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "terser": "^5.15.1",
    "vitest": "^0.24.5"
  },
  "directories": {
    "test": "test"
  }
}
